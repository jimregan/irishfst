SELECT (n) IF (0 ("comhair")) (-2 (pr)) (-1 (det pos));

# dála an scéil
SELECT (n) IF (0 ("dála")) (2 ("<scéil>"));

# mo/do/a dhála féin, ár ndála féin
SELECT (n) IF (0 ("dála")) (-1 (det pos));

