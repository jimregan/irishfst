#SELECT (vbser) IF (0 ("bí")) (T:BI-TAIL);
# TEMPLATELESS VERSION below.
SELECT (vbser) IF (0 ("bí" ecl)) (-1 (vpart itg) OR (vpart itg neg) OR (vpart rel neg) OR (vpart rel indirect) OR (Prep Rel) OR (cnjsub) OR (adv itg));
