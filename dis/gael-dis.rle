REMOVE (len) IF (0 ("léann"))(-1 ("mac"));# mac léinn

# leath uair=Com a chloig, 
# vs. leath an ama=Gen
SELECT (det) IF (0 ("leath")) (1 (n nom));

# níos mó ama, níos lú ama ...
SELECT (n gen) IF (-1 ("<mó>") OR ("<Lú>"));

# ar mhaith leat; is maith liom etc.
